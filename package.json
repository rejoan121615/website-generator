{
  "name": "website-generator",
  "private": true,
  "scripts": {
    "compile": "turbo run compile --filter=\"./packages/*\"",
    "build": "turbo run build --filter=\"./packages/*\"",
    "generate": "pnpm --filter @repo/app-generator start && pnpm install && turbo run build --filter=\"./apps/*\"",
    "generate:single": "pnpm --filter @repo/app-generator generate:single",
    "preview": "turbo run preview --filter=\"./apps/*\"",
    "preview:single": "pnpm --filter @repo/app-generator project:preview",
    "deploy": "turbo run deploy --filter=\"./apps/*\"",
    "deploy:single": "pnpm --filter @repo/cf deploy:single",
    "undeploy": "turbo run remove --filter=\"./apps/*\"",
    "undeploy:single": "pnpm --filter @repo/cf undeploy:single",
    "delete" : "pnpm --filter @repo/app-generator remove:all",
    "delete:single" : "pnpm --filter @repo/app-generator remove:single",
    "dashboard": "pnpm --filter @repo/dashboard start",
    "ready" : "pnpm install && pnpm run build"
  },
  "devDependencies": {
    "pnpm": "^10.16.1",
    "prettier": "^3.6.2",
    "turbo": "^2.5.6",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@10.17.1",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  },
  "pnpm": {
    "overrides": {
      "@types/node": "^24.5.0"
    }
  },
  "dependencies": {
    "wrangler": "^4.37.1"
  }
}
