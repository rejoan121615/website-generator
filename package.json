{
  "name": "website-generator",
  "private": true,
  "scripts": {
    "compile": "turbo run compile --filter=\"./packages/*\"",
    "build": "turbo run build --filter=\"./packages/*\"",
    "generate": "pnpm --filter @repo/app-generator start && pnpm install",
    "generate:single": "pnpm --filter @repo/app-generator single",
    "deploy": "turbo run deploy --filter=\"./apps/*\"",
    "deploy:single": "turbo run single --filter=\"./packages/app-generator\"",


    "preview": "turbo run preview --filter=\"./apps/*\"",
    "site:build": "turbo run build --filter=\"./apps/*\"",
    "site:remove": "turbo run remove --filter=\"./apps/*\"",
    "project:build" : "pnpm install && pnpm run build:packages && pnpm --filter @repo/dashboard make",
    "dashboard": "pnpm --filter @repo/dashboard start"
  },
  "devDependencies": {
    "pnpm": "^10.16.1",
    "prettier": "^3.6.2",
    "turbo": "^2.5.6",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@10.17.1",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  },
  "pnpm": {
    "overrides": {
      "@types/node": "^24.5.0"
    }
  },
  "dependencies": {
    "wrangler": "^4.37.1"
  }
}
