{
  "name": "website-generator",
  "private": true,
  "scripts": {
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "compile": "turbo run compile",
    "compile:apps": "turbo run compile --filter=\"./apps/*\"",
    "compile:packages": "turbo run compile --filter=\"./packages/*\"",
    "build": "turbo build",
    "build:apps": "turbo run build --filter=\"./apps/*\"",
    "build:packages": "turbo run build --filter=\"./packages/*\"",
    "dev": "turbo dev",
    "dev:apps": "turbo run dev --filter=\"./apps/*\"",
    "dev:packages": "turbo run dev --filter=\"./packages/*\"",
    "output": "turbo output",
    "output:packages": "turbo run output --filter=\"./packages/*\"",
    "site:generate": "",
    "site:build": "npm --workspace apps/website run build",
    "site:deploy": "npm --workspace apps/website run deploy",
    "generate": "npm run generate:run & npm run apps:build & npm run apps:deploy"
  },
  "devDependencies": {
    "pnpm": "^10.16.1",
    "prettier": "^3.6.2",
    "turbo": "^2.5.6",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@10.0.0",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  }
}
