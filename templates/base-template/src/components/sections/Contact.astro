---
import Form from "../ui/Form.astro";
import SectionTitle from "../ui/SectionTitle.astro";
import Section from "./Section.astro";

const sectionTitle = "[[Contact Us|Get in Touch|Reach Out]]";
const description = "[[Connect with Us|Reach out to us|Get in touch]]: Let's Discuss Your [[Electrical|Power]] Needs";
---

<Section id="contact">
  <div class="mb-[140px] relative">
    <SectionTitle
      sectionTitle={sectionTitle}
      description={description}
    />
  </div>
</Section>
